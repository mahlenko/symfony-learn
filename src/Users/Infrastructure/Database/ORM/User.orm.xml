<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Users\Domain\Entity\User" table="users_user">
        <id name="ulid" type="string" length="20">
            <generator strategy="NONE"/>
        </id>
        <field name="email" type="string" length="50"/>
        <field name="password" type="string" length="50"/>
        <field name="created_at" type="date_immutable"/>
    </entity>
</doctrine-mapping>